{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES6",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "types": ["vite/client", "node", "unplugin-vue-define-options/macros-global"],
    "incremental": false,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "paths": {
      "@/*": ["src/*"],
      "#vo/*": ["Vo/*"]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "types/**/*.d.ts",
    "build/**/*.ts",
    "auto-imports.d.ts",
    "vite.config.ts",
    "package.json",
    "electron",
    "Vo",
    "enums",
    "types/**/*.d.ts"
  ],
  "exclude": ["dist", "node_modules"],
}
